{
  "compilerOptions": {
    "module": "nodenext",
    "moduleResolution": "bundler",
    "target": "esnext",
    "sourceMap": true,
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "incremental": true,
    "noUnusedLocals": true
  },
  "exclude": [
    "node_modules",
  ],
}
